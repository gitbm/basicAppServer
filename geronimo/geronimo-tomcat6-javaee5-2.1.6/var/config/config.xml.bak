    <module name="org.apache.geronimo.configs/mejb/2.1.6/car"/>
    <module name="org.apache.geronimo.configs/dojo-tomcat/2.1.6/car"/>
    <module name="org.apache.geronimo.plugins.monitoring/agent-car-jmx/2.1.6/car"/>
    <module name="org.apache.geronimo.plugins/agent-ds/2.1.6/car"/>
    <module name="org.apache.geronimo.plugins.monitoring/agent-car-ejb/2.1.6/car" load="false"/>
    <module name="org.apache.geronimo.plugins/mconsole-tomcat/2.1.6/car"/>
    <module name="org.apache.geronimo.plugins/mconsole-ds/2.1.6/car"/>
    <module name="org.apache.geronimo.framework/jmx-security/2.1.6/car" load="false">
        <gbean name="JMXSecureConnector">
            <attribute name="protocol">rmi</attribute>
            <attribute name="host">${ServerHostname}</attribute>
            <attribute name="port">${JMXSecurePort + PortOffset}</attribute>
            <attribute name="urlPath">/jndi/rmi://${ServerHostname}:${NamingPort + PortOffset}/JMXSecureConnector</attribute>
            <attribute name="algorithm">Default</attribute>
            <attribute name="secureProtocol">TLS</attribute>
            <attribute name="keyStore">geronimo-default</attribute>
            <attribute name="keyAlias">geronimo</attribute>
            <attribute name="trustStore">geronimo-default</attribute>
            <attribute name="clientAuth">false</attribute>
        </gbean>
    </module>
</attributes>
