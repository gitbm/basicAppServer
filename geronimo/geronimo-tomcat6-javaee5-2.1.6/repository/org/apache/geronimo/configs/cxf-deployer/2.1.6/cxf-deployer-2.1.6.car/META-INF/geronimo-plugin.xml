<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<geronimo-plugin xmlns="http://geronimo.apache.org/xml/ns/plugins-1.3" xmlns:ns2="http://geronimo.apache.org/xml/ns/attributes-1.2">
    <name>Geronimo Plugins, CXF :: Deployer</name>
    <category>Web Services</category>
    <description>Geronimo JAX-WS deployer for Apache CXF</description>
    <url>http://geronimo.apache.org/</url>
    <author>The Apache Geronimo development community</author>
    <license osi-approved="true">The Apache Software License, Version 2.0</license>
    <plugin-artifact>
        <module-id>
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>cxf-deployer</artifactId>
            <version>2.1.6</version>
            <type>car</type>
        </module-id>
        <geronimo-version>2.1.6</geronimo-version>
        <jvm-version>1.5</jvm-version>
        <jvm-version>1.6</jvm-version>
        <dependency>
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>jaxws-deployer</artifactId>
            <version>2.1.6</version>
            <type>car</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>cxf</artifactId>
            <version>2.1.6</version>
            <type>car</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-cxf-builder</artifactId>
            <version>2.1.6</version>
            <type>jar</type>
        </dependency>
        <source-repository>~/.m2/repository/</source-repository>
        <source-repository>http://repo1.maven.org/maven2/</source-repository>
        <source-repository>http://repository.apache.org/snapshots/</source-repository>
        <source-repository>http://people.apache.org/repo/m2-incubating-repository/</source-repository>
        <config-xml-content server="default" condition="props['org.apache.geronimo.jaxws.provider'] == 'cxf' or (props['org.apache.geronimo.jaxws.provider'] == null and webcontainerName == 'jetty6')"/>
        <config-xml-content server="offline" condition="props['org.apache.geronimo.jaxws.provider'] == 'cxf' or (props['org.apache.geronimo.jaxws.provider'] == null and webcontainerName == 'jetty6')"/>
        <artifact-alias key="org.apache.geronimo.configs/cxf-deployer//car">org.apache.geronimo.configs/cxf-deployer/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/cxf-deployer/2.1.4/car">org.apache.geronimo.configs/cxf-deployer/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/cxf-deployer/2.1.3/car">org.apache.geronimo.configs/cxf-deployer/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/cxf-deployer/2.1.2/car">org.apache.geronimo.configs/cxf-deployer/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/cxf-deployer/2.1.1/car">org.apache.geronimo.configs/cxf-deployer/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/cxf-deployer/2.1/car">org.apache.geronimo.configs/cxf-deployer/2.1.6/car</artifact-alias>
    </plugin-artifact>
</geronimo-plugin>
