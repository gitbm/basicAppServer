<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<geronimo-plugin xmlns="http://geronimo.apache.org/xml/ns/plugins-1.3" xmlns:ns2="http://geronimo.apache.org/xml/ns/attributes-1.2">
    <name>Geronimo Plugins, JavaMail :: JavaMail</name>
    <category>JavaEE</category>
    <description>Geronimo JavaMail support</description>
    <url>http://geronimo.apache.org/</url>
    <author>The Apache Geronimo development community</author>
    <license osi-approved="true">The Apache Software License, Version 2.0</license>
    <plugin-artifact>
        <module-id>
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>javamail</artifactId>
            <version>2.1.6</version>
            <type>car</type>
        </module-id>
        <geronimo-version>2.1.6</geronimo-version>
        <jvm-version>1.5</jvm-version>
        <jvm-version>1.6</jvm-version>
        <dependency>
            <groupId>org.apache.geronimo.framework</groupId>
            <artifactId>rmi-naming</artifactId>
            <version>2.1.6</version>
            <type>car</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.javamail</groupId>
            <artifactId>geronimo-javamail_1.4_mail</artifactId>
            <version>1.7</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-mail</artifactId>
            <version>2.1.6</version>
            <type>jar</type>
        </dependency>
        <source-repository>~/.m2/repository/</source-repository>
        <source-repository>http://repo1.maven.org/maven2/</source-repository>
        <source-repository>http://repository.apache.org/snapshots/</source-repository>
        <source-repository>http://people.apache.org/repo/m2-incubating-repository/</source-repository>
        <config-xml-content>
            <ns2:gbean name="SMTPTransport">
                <ns2:attribute name="host">${SMTPHost}</ns2:attribute>
                <ns2:attribute name="port">${SMTPPort}</ns2:attribute>
            </ns2:gbean>
        </config-xml-content>
        <artifact-alias key="org.apache.geronimo.configs/javamail//car">org.apache.geronimo.configs/javamail/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/javamail/2.1.4/car">org.apache.geronimo.configs/javamail/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/javamail/2.1.3/car">org.apache.geronimo.configs/javamail/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/javamail/2.1.2/car">org.apache.geronimo.configs/javamail/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/javamail/2.1.1/car">org.apache.geronimo.configs/javamail/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/javamail/2.1/car">org.apache.geronimo.configs/javamail/2.1.6/car</artifact-alias>
        <config-substitution key="SMTPHost">localhost</config-substitution>
        <config-substitution key="SMTPPort">25</config-substitution>
    </plugin-artifact>
</geronimo-plugin>
