    <module name="org.apache.geronimo.configs/jasper-deployer/2.1.6/car"/>
    <module name="org.apache.geronimo.configs/myfaces/2.1.6/car"/>
    <module name="org.apache.geronimo.configs/myfaces-deployer/2.1.6/car"/>
