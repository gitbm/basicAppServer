    <module name="org.apache.geronimo.configs/axis2-ejb-deployer/2.1.6/car" condition="props['org.apache.geronimo.jaxws.provider'] == 'axis2' or (props['org.apache.geronimo.jaxws.provider'] == null and webcontainerName == 'tomcat6')"/>
