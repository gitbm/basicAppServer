<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<geronimo-plugin xmlns="http://geronimo.apache.org/xml/ns/plugins-1.3" xmlns:ns2="http://geronimo.apache.org/xml/ns/attributes-1.2">
    <name>Geronimo Plugins, J2EE :: Deployer</name>
    <category>JavaEE deployment</category>
    <description>Basic JavaEE deployer functionality including the ear deployer.</description>
    <url>http://geronimo.apache.org/</url>
    <author>The Apache Geronimo development community</author>
    <license osi-approved="true">The Apache Software License, Version 2.0</license>
    <plugin-artifact>
        <module-id>
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>j2ee-deployer</artifactId>
            <version>2.1.6</version>
            <type>car</type>
        </module-id>
        <geronimo-version>2.1.6</geronimo-version>
        <jvm-version>1.5</jvm-version>
        <jvm-version>1.6</jvm-version>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-j2ee-schema</artifactId>
            <version>2.1.6</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.schema</groupId>
            <artifactId>geronimo-schema-jee_5</artifactId>
            <version>1.1</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.schema</groupId>
            <artifactId>geronimo-schema-j2ee_1.4</artifactId>
            <version>1.2</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.configs</groupId>
            <artifactId>j2ee-server</artifactId>
            <version>2.1.6</version>
            <type>car</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-test-ddbean</artifactId>
            <version>2.1.6</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-naming-builder</artifactId>
            <version>2.1.6</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-security-builder</artifactId>
            <version>2.1.6</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.framework</groupId>
            <artifactId>server-security-config</artifactId>
            <version>2.1.6</version>
            <type>car</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-j2ee-builder</artifactId>
            <version>2.1.6</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.framework</groupId>
            <artifactId>geronimo-gbean-deployer</artifactId>
            <version>2.1.6</version>
            <type>car</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-client</artifactId>
            <version>2.1.6</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.xbean</groupId>
            <artifactId>xbean-finder</artifactId>
            <version>3.6</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.apache.geronimo.modules</groupId>
            <artifactId>geronimo-web-2.5-builder</artifactId>
            <version>2.1.6</version>
            <type>jar</type>
        </dependency>
        <source-repository>~/.m2/repository/</source-repository>
        <source-repository>http://repo1.maven.org/maven2/</source-repository>
        <source-repository>http://repository.apache.org/snapshots/</source-repository>
        <source-repository>http://people.apache.org/repo/m2-incubating-repository/</source-repository>
        <config-xml-content>
            <ns2:gbean name="WebBuilder">
                <ns2:attribute name="defaultNamespace">http://geronimo.apache.org/xml/ns/j2ee/web/jetty-2.0</ns2:attribute>
            </ns2:gbean>
            <ns2:gbean name="EnvironmentEntryBuilder">
                <ns2:attribute name="eeNamespaces">http://java.sun.com/xml/ns/j2ee,http://java.sun.com/xml/ns/javaee</ns2:attribute>
            </ns2:gbean>
        </config-xml-content>
        <config-xml-content server="offline">
            <ns2:gbean name="WebBuilder">
                <ns2:attribute name="defaultNamespace">http://geronimo.apache.org/xml/ns/j2ee/web/jetty-2.0</ns2:attribute>
            </ns2:gbean>
            <ns2:gbean name="EnvironmentEntryBuilder">
                <ns2:attribute name="eeNamespaces">http://java.sun.com/xml/ns/j2ee,http://java.sun.com/xml/ns/javaee</ns2:attribute>
            </ns2:gbean>
        </config-xml-content>
        <artifact-alias key="org.apache.geronimo.configs/j2ee-deployer//car">org.apache.geronimo.configs/j2ee-deployer/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/j2ee-deployer/2.1.4/car">org.apache.geronimo.configs/j2ee-deployer/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/j2ee-deployer/2.1.3/car">org.apache.geronimo.configs/j2ee-deployer/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/j2ee-deployer/2.1.2/car">org.apache.geronimo.configs/j2ee-deployer/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/j2ee-deployer/2.1.1/car">org.apache.geronimo.configs/j2ee-deployer/2.1.6/car</artifact-alias>
        <artifact-alias key="org.apache.geronimo.configs/j2ee-deployer/2.1/car">org.apache.geronimo.configs/j2ee-deployer/2.1.6/car</artifact-alias>
    </plugin-artifact>
</geronimo-plugin>
