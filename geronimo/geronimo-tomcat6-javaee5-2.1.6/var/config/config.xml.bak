    <module name="org.apache.geronimo.framework/client-system/2.1.6/car" load="false"/>
    <module name="org.apache.geronimo.framework/online-deployer/2.1.6/car" load="false"/>
    <module name="org.apache.geronimo.framework/gshell-geronimo/2.1.6/car"/>
